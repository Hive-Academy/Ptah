{
  "mcpServers": {
    "browser": {
      "command": "cmd.exe",
      "args": ["/c", "npx", "-y", "@agent-infra/mcp-server-browser@latest"]
    },

    "sequential-thinking": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },

    "nx-mcp": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "C://Program Files/nodejs/npx.cmd",
        "-y",
        "nx-mcp@latest",
        "D://projects/Ptah"
      ]
    },

    "angular-cli": {
      "command": "cmd.exe",
      "args": ["/c", "npx", "-y", "@angular/cli", "mcp"]
    },

    "neo4j-graph-data-modeling": {
      "command": "uvx",
      "args": ["mcp-neo4j-data-modeling@0.1.1"],
      "disabled": true
    },

    "chroma": {
      "command": "uvx",
      "args": [
        "chroma-mcp",
        "--client-type",
        "persistent",
        "--data-dir",
        "/data/chromadb"
      ],
      "disabled": true
    }
  }
}
